{
  "socket": {
    "path": "claude_controller.sock",
    "timeout": 1.0
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s [%(levelname)s] %(message)s"
  },
  "process": {
    "termination_timeout": 5
  },
  "claude_runner": {
    "dangerously_skip_permissions": true,
    "default_model": null,
    "node_setup": {
      "type": "auto",
      "nvm_dir": null,
      "nvm_script": null,
      "node_path": null,
      "claude_path": null
    }
  }
}